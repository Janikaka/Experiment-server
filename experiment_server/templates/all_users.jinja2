{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
	<h1>All users:</h1><br>
	<ul>
	{% for user in users %}
		<li class="font-bold">{{ user.username }}<br>
		<a href="">Show user</a><br>
		<a href="/users/{{user.id}}/experiments">Show experiments</a><br>
		</li>
		<br>
	{% endfor %}
	</ul>
	<br>
	<a href="/experiments">Show all experiments</a>
</div>
{% endblock content %}

