{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
	<h1>All experiments:</h1>
	<ul>
	{% for experiment in experiments %}
		<li class="font-bold">Experiment name: <p>{{ experiment.name }}</p>
			Experiment groups:
			<ul>
			{% for experimentgroup in experiment.experimentgroups %}
				<li><p>{{ experimentgroup.name }}</p></li>
			{% endfor %}
			</ul>
		</li>
		<br>
	{% endfor %}
	</ul>
</div>
{% endblock content %}



