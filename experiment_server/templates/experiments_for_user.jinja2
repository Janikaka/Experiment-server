{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
	<h1>All experiments user {{ user.username }} participates:</h1><br>
	<ul>
	{% for experiment in experiments %}
		<li class="font-bold">ID: {{experiment.id}} </li>
		<li class="font-bold"><a href="/experiments/{{experiment.id}}/metadata">{{ experiment.name }}</a>
		</li>
		<br>
	{% endfor %}
	</ul>
</div>
{% endblock content %}

