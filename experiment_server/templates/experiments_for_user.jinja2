{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
	<h1>All experiments <i>{{ user.username }}</i> participates:</h1><br>
	<ul>
	{% for experiment in experiments %}
		<li class="font-bold">
		<a href="/experiments/{{experiment.id}}/metadata">{{ experiment.name }}</a>
		</li>
		<br>
	{% endfor %}
	</ul>
	<br>
	<input type="button" value="Back" onclick="window.history.back()"/>
</div>
{% endblock content %}

