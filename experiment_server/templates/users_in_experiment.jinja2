{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
	<h1>All users in {{ experiment.name }}:</h1><br>
	<ul>
	{% for user in users %}
		<li class="font-bold">ID: {{user.id}} </li>
		<li class="font-bold"><a>{{ user.username }}</a>
		</li>
		<br>
	{% endfor %}
	</ul>
</div>
{% endblock content %}

