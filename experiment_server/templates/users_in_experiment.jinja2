{% extends "layout.jinja2" %}

{% block content %}
<div class="content">
	<h1>All users in <i>{{ experiment.name }}:</i></h1><br>
	<ul>
	{% for user in users %}
		<li class="font-bold">
		<a href='/users/{{user.id}}'>{{ user.username }}</a><br>
		</li>
		<br>
	{% endfor %}
	</ul>
	<br>
	<input type="button" value="Back" onclick="window.history.back()"/>
</div>
{% endblock content %}

