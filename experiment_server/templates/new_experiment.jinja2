{% extends "layout.jinja2" %}

{% block content %}
<form action="{{ url }}" method=post>
    <dl>
      <dt>Name:
      <dd><input type=text name='name' placeholder='Enter experiment name'><br><br>
      <dt>Group1:
      <dd><input type=text name='group1' placeholder='Enter experimentgroup'>
      <dd><input type=text name='group1key' placeholder='Enter conf key'>
      <dd><input type=text name='group1value' placeholder='Enter conf value'><br><br>
      <dt>Group2:
      <dd><input type=text name='group2' placeholder='Enter experimentgroup'>
      <dd><input type=text name='group2key' placeholder='Enter conf key'>
      <dd><input type=text name='group2value' placeholder='Enter conf value'><br><br>
      <dd><input type=submit value=Create>
    </dl>
</form>

{% endblock content %}






